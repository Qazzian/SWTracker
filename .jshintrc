{
  "browser": true,
  "jquery": true,
  "node": true,
  "curly": true,
  "esnext": true,
  "eqeqeq": true,
  "forin": true,
  "freeze": true,
  "indent": 4,
  "latedef": true,
  "immed": true,
  "noempty": true,
  "newcap": true,
  "nonew": false,
  "quotmark": "single",
  "strict": true,
  "undef": true,
  "unused": true,

  "globals": { "ACC": true },

  // RELAXING OPTIONS
  "eqnull": true,

  "predef" : [
    // RequireJS globals
	"define",
	"require",
	"requirejs",
    // Jasmin globals
	"describe",
	"it",
	"expect",
	"beforeEach",
	"afterEach",
	"spyOn",
	"jasmine",
	"xit"
  ]

}


